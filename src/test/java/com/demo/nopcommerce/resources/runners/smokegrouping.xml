<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Nopcommerce-Smoke Test" verbose="1" >
    <groups>
        <run>
            <include name="smoke"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.demo.nopcommerce.listeners.Listeners"></listener>
    </listeners>
    <test name="Smoke Test" >
        <classes>
            <class name="com.demo.nopcommerce.testsuit.LoginTestWithDataProvider"/>
            <class name="com.demo.nopcommerce.testsuit.ComputerPageTest"/>
            <class name="com.demo.nopcommerce.testsuit.RegistrationTestWithDataProvider"/>

        </classes>
    </test>
</suite>