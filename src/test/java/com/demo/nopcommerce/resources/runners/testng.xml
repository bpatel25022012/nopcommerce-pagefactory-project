<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Nopcommerce Demo Test" verbose="1" >
    <listeners>
        <listener class-name="com.demo.nopcommerce.listeners.Listeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="Login Test" >
        <classes>
            <class name="com.demo.nopcommerce.testsuit.LoginTestWithDataProvider" />
        </classes>
    </test>
    <test name="Registration Test">
        <classes>
            <class name="com.demo.nopcommerce.testsuit.RegistrationTestWithDataProvider" />
        </classes>
    </test>
    <test name="ComputerPage Test" >
        <classes>
            <class name="com.demo.nopcommerce.testsuit.ComputerPageTest" />
        </classes>
    </test>
</suite>
